    "b4a": "^1.7.3",
    "blind-pairing": "^2.3.1",
    "compact-encoding": "^2.18.0",
    "corestore": "^7.7.0",
    "hyperbee": "^2.26.5",
    "hyperswarm": "^4.16.0",
    "random-access-memory": "^6.2.1",
    "ready-resource": "^1.2.0",
    "z32": "^1.1.0"
  },
